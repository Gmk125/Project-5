def open_worksheet():
    Wb = openpyxl.load.workbook("countyPopChange2020-2021")
    worksheet = Wb.active
    for i in worksheet.iter_rows(min_row=1, max_col=worksheet.max)
        data = row[i].value
    return data
    
def should_display_pop_change():
    UserInput=input("should the map display total population changes")
    if UserInput=="yes":
         return True
    elif UserInput=="no"
         returnfalse
         
         
